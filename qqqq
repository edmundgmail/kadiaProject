SELECT
USER_ID                       ,
USER_NAME                     ,
LAST_UPDATE_DATE              ,
LAST_UPDATED_BY               ,
CREATION_DATE                 ,
CREATED_BY                    ,
LAST_UPDATE_LOGIN             ,
ENCRYPTED_FOUNDATION_PASSWORD ,
ENCRYPTED_USER_PASSWORD       ,
SESSION_Double                ,
START_DATE                    ,
END_DATE                      ,
DESCRIPTION                   ,
LAST_LOGON_DATE               ,
PASSWORD_DATE                 ,
PASSWORD_ACCESSES_LEFT        ,
PASSWORD_LIFESPAN_ACCESSES    ,
PASSWORD_LIFESPAN_DAYS        ,
EMPLOYEE_ID                   ,
EMAIL_ADDRESS                 ,
FAX                           ,
CUSTOMER_ID                   ,
SUPPLIER_ID                   ,
WEB_PASSWORD                  ,
USER_GUID                     ,
GCN_CODE_COMBINATION_ID       ,
PERSON_PARTY_ID               
FROM FN_USER
WHERE LAST_UPDATE_DATE > TO_DATE(transfer_date, 'DD-MON-RRRR HH24:MI:SS')

CREATE EXTERNAL TABLE FN_USER (
USER_ID                       BIGINT    ,
USER_NAME                     VARCHAR(100) ,
LAST_UPDATE_DATE              TIMESTAMP          ,
LAST_UPDATED_BY               BIGINT    ,
CREATION_DATE                 TIMESTAMP          ,
CREATED_BY                    BIGINT    ,
LAST_UPDATE_LOGIN             BIGINT    ,
ENCRYPTED_FOUNDATION_PASSWORD VARCHAR(100) ,
ENCRYPTED_USER_PASSWORD       VARCHAR(100) ,
SESSION_Double                Double        ,
START_DATE                    TIMESTAMP          ,
END_DATE                      TIMESTAMP          ,
DESCRIPTION                   VARCHAR(240) ,
LAST_LOGON_DATE               TIMESTAMP          ,
PASSWORD_DATE                 TIMESTAMP          ,
PASSWORD_ACCESSES_LEFT        BIGINT    ,
PASSWORD_LIFESPAN_ACCESSES    BIGINT    ,
PASSWORD_LIFESPAN_DAYS        BIGINT    ,
EMPLOYEE_ID                   BIGINT    ,
EMAIL_ADDRESS                 VARCHAR(240) ,
FAX                           VARCHAR(80)  ,
CUSTOMER_ID                   BIGINT    ,
SUPPLIER_ID                   BIGINT    ,
WEB_PASSWORD                  VARCHAR(240) ,
USER_GUID                     BINARY(16)  ,
GCN_CODE_COMBINATION_ID       BIGINT    ,
PERSON_PARTY_ID               Double)
PARTITIONED BY (ds string)
ROW FORMAT DELIMITED FIELDS TERMINATED BY ','
LOCATION '/user/cloudera/staging/FN_USER'

SELECT
ROW_ID             ,
ATTRIBUTE_CATEGORY ,
ATTRIBUTE1         ,
ATTRIBUTE2         ,
ATTRIBUTE3         ,
ATTRIBUTE4         ,
ATTRIBUTE5         ,
TERRITORY_CODE     ,
ATTRIBUTE6         ,
ATTRIBUTE7         ,
ATTRIBUTE8         ,
ATTRIBUTE9         ,
ATTRIBUTE10        ,
ATTRIBUTE11        ,
ATTRIBUTE12        ,
ATTRIBUTE13        ,
ATTRIBUTE14        ,
ATTRIBUTE15        ,
LOOKUP_TYPE        ,
LOOKUP_CODE        ,
ENABLED_FLAG       ,
START_DATE_ACTIVE  ,
END_DATE_ACTIVE    ,
CREATED_BY         ,
CREATION_DATE      ,
LAST_UPDATED_BY    ,
LAST_UPDATE_LOGIN  ,
LAST_UPDATE_DATE   ,
SECURITY_GROUP_ID  ,
VIEW_APPLICATION_ID,
TAG                ,
MEANING            ,
DESCRIPTION        
FROM FND_LOOKUP_VALUES_VL,
WHERE LAST_UPDATE_DATE > TO_DATE(transfer_date, 'DD-MON-RRRR HH24:MI:SS')


CREATE EXTERNAL TABLE FND_LOOKUP_VALUES_VL (
ROW_ID               BIGINT         ,
ATTRIBUTE_CATEGORY   VARCHAR(30)  ,
ATTRIBUTE1           VARCHAR(150) ,
ATTRIBUTE2           VARCHAR(150) ,
ATTRIBUTE3           VARCHAR(150) ,
ATTRIBUTE4           VARCHAR(150) ,
ATTRIBUTE5           VARCHAR(150) ,
TERRITORY_CODE       VARCHAR(2)   ,
ATTRIBUTE6           VARCHAR(150) ,
ATTRIBUTE7           VARCHAR(150) ,
ATTRIBUTE8           VARCHAR(150) ,
ATTRIBUTE9           VARCHAR(150) ,
ATTRIBUTE10          VARCHAR(150) ,
ATTRIBUTE11          VARCHAR(150) ,
ATTRIBUTE12          VARCHAR(150) ,
ATTRIBUTE13          VARCHAR(150) ,
ATTRIBUTE14          VARCHAR(150) ,
ATTRIBUTE15          VARCHAR(150) ,
LOOKUP_TYPE          VARCHAR(30)  ,
LOOKUP_CODE          VARCHAR(30)  ,
ENABLED_FLAG         VARCHAR(1)   ,
START_DATE_ACTIVE    TIMESTAMP          ,
END_DATE_ACTIVE      TIMESTAMP          ,
CREATED_BY           BIGINT    ,
CREATION_DATE        TIMESTAMP          ,
LAST_UPDATED_BY      BIGINT    ,
LAST_UPDATE_LOGIN    BIGINT    ,
LAST_UPDATE_DATE     TIMESTAMP          ,
SECURITY_GROUP_ID    BIGINT    ,
VIEW_APPLICATION_ID  BIGINT    ,
TAG                  VARCHAR(150) ,
MEANING              VARCHAR(80)  ,
DESCRIPTION          VARCHAR(240))
PARTITIONED BY (ds string)
ROW FORMAT DELIMITED FIELDS TERMINATED BY ','
LOCATION '/user/cloudera/staging/FND_LOOKUP_VALUES_VL'

