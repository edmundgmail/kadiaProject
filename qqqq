WH_UPDATE_DATE               TIMESTAMP          ,
PERSON_ID                    Double        ,
SALES_TAX_GEOCODE            VARCHAR(30)  ,
SALES_TAX_INSIDE_CITY_LIMITS VARCHAR(1)   ,
OBJECT_VERSION_NUMBER        Double        ,
ATTRIBUTE_CATEGORY           VARCHAR(30)  ,
ATTRIBUTE1                   VARCHAR(150) ,
ATTRIBUTE2                   VARCHAR(150) ,
ATTRIBUTE3                   VARCHAR(150) ,
ATTRIBUTE4                   VARCHAR(150) ,
ATTRIBUTE5                   VARCHAR(150) ,
